{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 8 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8",
            "exclude": [
                "drupal/a11y",
                "drupal/entity_submit_redirect",
                "drupal/field_slideshow"
            ]
        },
        {
            "type": "package",
            "package": {
                "name": "drupal/a11y",
                "version": "dev-custom",
                "type": "drupal-module",
                "source": {
                    "type": "git",
                    "url": "https://git.drupalcode.org/issue/a11y-3364136.git",
                    "reference": "3364136-drupal-10-update"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "drupal/entity_submit_redirect",
                "version": "dev-custom",
                "type": "drupal-module",
                "source": {
                    "type": "git",
                    "url": "https://git.drupalcode.org/issue/entity_submit_redirect-3353587.git",
                    "reference": "8.x-1.x"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "drupal/field_slideshow",
                "version": "dev-custom",
                "type": "drupal-module",
                "source": {
                    "type": "git",
                    "url": "https://git.drupalcode.org/issue/field_slideshow-3341466.git",
                    "reference": "3341466-drupal-10-compatibility"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "recurser/jquery-simple-color",
                "version": "1.2.3",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/recurser/jquery-simple-color",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "bgrins/spectrum",
                "version": "1.8.1",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/bgrins/spectrum",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "davatron5000/fitvids",
                "version": "1.0",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/davatron5000/FitVids.js",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "bgrins/spectrum": "^1.8",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "~1.0",
        "davatron5000/fitvids": "^1.0",
        "drupal/a11y": "dev-custom",
        "drupal/address": "^2.0",
        "drupal/address_map_link": "^1.4",
        "drupal/admin_toolbar": "^3.4",
        "drupal/all_in_one_accessibility": "^1.5",
        "drupal/auto_entitylabel": "^3.0",
        "drupal/background_image_formatter": "^2.0",
        "drupal/backup_migrate": "^5.0",
        "drupal/bg_image_formatter": "^1.17",
        "drupal/block_list_override": "^1.0",
        "drupal/block_permissions": "^1.3",
        "drupal/bootstrap_barrio": "^5.5",
        "drupal/bootstrap_ce": "^2.1",
        "drupal/bootstrap_layouts": "^5.3",
        "drupal/bootstrap_paragraphs": "^2.0@beta",
        "drupal/bootstrap_simple_carousel": "^2.0",
        "drupal/ckeditor5_paste_filter": "^1.1",
        "drupal/ckeditor5_plugin_pack": "^1.3",
        "drupal/ckeditor_blockimagepaste": "^2.0",
        "drupal/ckeditor_bootstrap_table": "^1.3",
        "drupal/ckeditor_font": "^1.4",
        "drupal/ckeditor_media_resize": "^1.0",
        "drupal/ckwordcount": "^2.1",
        "drupal/color": "^2.0",
        "drupal/color_field": "^3.0",
        "drupal/colorbox": "^2.0",
        "drupal/colorbutton": "^1.5",
        "drupal/config_filter": "^2.6",
        "drupal/config_ignore": "^3.2",
        "drupal/config_split": "^2.0",
        "drupal/config_terms": "^1.9",
        "drupal/context": "^5.0@RC",
        "drupal/core-composer-scaffold": "^10",
        "drupal/core-project-message": "^10",
        "drupal/core-recommended": "^10",
        "drupal/crop": "^2.3",
        "drupal/ctools": "^3.14",
        "drupal/customerror": "^1.0@beta",
        "drupal/dbug": "^2.1",
        "drupal/devel": "^5.0",
        "drupal/disable_messages": "^2.1",
        "drupal/display_field_copy": "^2.1",
        "drupal/ds": "^3.15",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/editor_advanced_link": "^2.2",
        "drupal/entity_redirect": "^2.2",
        "drupal/entity_reference_revisions": "^1.11",
        "drupal/entity_submit_redirect": "dev-custom",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/eva": "^3.0",
        "drupal/feeds": "^3.0@beta",
        "drupal/feeds_ical": "^3.0",
        "drupal/feeds_tamper": "^2.0@beta",
        "drupal/field_group": "^4.0",
        "drupal/field_group_link": "^3.1",
        "drupal/field_permissions": "^1.3",
        "drupal/field_slideshow": "dev-custom",
        "drupal/field_token_value": "^3.0",
        "drupal/file_delete": "^3.0",
        "drupal/fitvids": "^2.0",
        "drupal/flood_control": "^3.0",
        "drupal/focal_point": "^2.0",
        "drupal/fullcalendar_view": "^5.1",
        "drupal/gin": "^4.1",
        "drupal/google_analytics": "^4.0",
        "drupal/google_tag": "^2.0",
        "drupal/gtranslate": "^3.0",
        "drupal/honeypot": "^2.1",
        "drupal/image_effects": "^4.0",
        "drupal/imagefield_slideshow": "^2.0",
        "drupal/imagemagick": "^4.0",
        "drupal/jquery_ui": "^1.6",
        "drupal/libraries": "^4.0",
        "drupal/linkit": "^7.0",
        "drupal/maxlength": "^3.1",
        "drupal/media_entity_file_replace": "^1.1",
        "drupal/menu_block": "^1.10",
        "drupal/menu_link_attributes": "^1.3",
        "drupal/node_revision_delete": "^2.0@RC",
        "drupal/o365": "^5.0",
        "drupal/onlyone": "^2.0",
        "drupal/openid_connect": "^3.0",
        "drupal/openid_connect_windows_aad": "^2.0@beta",
        "drupal/panelbutton": "^1.5",
        "drupal/paragraph_view_mode": "^3.1",
        "drupal/paragraphs": "^1.17",
        "drupal/paragraphs_edit": "^3.0",
        "drupal/password_policy": "^4.0",
        "drupal/pathauto": "^1.12",
        "drupal/protected_pages": "^1.6",
        "drupal/queue_ui": "^3.1",
        "drupal/redirect": "^1.9",
        "drupal/redis": "^1.9",
        "drupal/required_api": "^2.1",
        "drupal/required_by_role": "^1.3",
        "drupal/restui": "^1.22",
        "drupal/scheduler": "^2.2",
        "drupal/seven": "^1.0",
        "drupal/simple_cron": "^1.1@beta",
        "drupal/simple_image_rotate": "^2.3@alpha",
        "drupal/smart_date": "^4.0",
        "drupal/smart_trim": "^2.1",
        "drupal/social_media_links": "2.x-dev@dev",
        "drupal/stage_file_proxy": "^3.1",
        "drupal/tabtamer": "^2.0@beta",
        "drupal/tamper": "^1.0@alpha",
        "drupal/taxonomy_manager": "^2.0",
        "drupal/term_merge": "^2.0@beta",
        "drupal/term_reference_change": "^2.0@beta",
        "drupal/theme_permission": "^2.0.0",
        "drupal/token": "^1.13",
        "drupal/twitter_block": "^4.0",
        "drupal/unpublished_paragraphs": "^1.4",
        "drupal/upgrade_status": "^4.1",
        "drupal/video_embed_field": "^3.0",
        "drupal/views_base_url": "^2.1@beta",
        "drupal/views_bootstrap": "^5.4",
        "drupal/views_bulk_operations": "^4.2",
        "drupal/views_conditional": "^1.9",
        "drupal/views_ical": "1.x-dev@dev",
        "drupal/views_menu_children_filter": "^3.0@RC",
        "drupal/views_sort_options_weight": "^2.1",
        "drupal/views_year_filter": "^2.1",
        "drupal/viewsreference": "^2.0@beta",
        "drupal/webform": "^6.2",
        "drupal/workbench_access": "^2.0",
        "drush/drush": "^13",
        "onlyextart/colorbox": "9999999-dev",
        "php-ews/php-ews": "^1.0",
        "platformsh/config-reader": "^2.4",
        "recurser/jquery-simple-color": "^1.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "patches": {
            "drupal/eva": {
                "Replace empty token to skip contextual filter": "patches/replace_an_empty_token.patch"
            },
            "drupal/field_group": {
                "2961885: Allow token to be used in attributes and classes for HTML elements": "https://www.drupal.org/files/issues/2025-02-04/field_group-2961885-26-mr96.diff"
            },
            "drupal/fullcalendar_view": {
                "3280982: FullviewCalendar With Repeating Smart Date Events Causing Significant Load Times": "https://www.drupal.org/files/issues/2023-08-11/fullcalendar_view_repeating_events_5.1.12.patch"
            },
            "drupal/openid_connect_windows_aad": {
                "3124961: Enforce HTTPS": "https://git.drupalcode.org/project/openid_connect_windows_aad/-/merge_requests/26.patch"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, you’ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
