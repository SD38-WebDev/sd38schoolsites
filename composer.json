{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 8 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
              "name": "recurser/jquery-simple-color",
              "version": "1.2.3",
              "type": "drupal-library",
              "source": {
                "url": "https://github.com/recurser/jquery-simple-color",
                "type": "git",
                "reference": "master"
              }
            }
          },
          {
              "type": "package",
              "package": {
                "name": "bgrins/spectrum",
                "version": "1.8.1",
                "type": "drupal-library",
                "source": {
                  "url": "https://github.com/bgrins/spectrum",
                  "type": "git",
                  "reference": "master"
                }
              }
            },
            {
                "type": "package",
                "package": {
                  "name": "davatron5000/fitvids",
                  "version": "1.0",
                  "type": "drupal-library",
                  "source": {
                    "url": "https://github.com/davatron5000/FitVids.js",
                    "type": "git",
                    "reference": "master"
                  }
                }
              }
    ],
    "require": {
        "bgrins/spectrum": "^1.8",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "~1.0",
        "davatron5000/fitvids": "^1.0",
        "drupal/address": "^1.9",
        "drupal/address_map_link": "^1.2",
        "drupal/admin_toolbar": "^3.0",
        "drupal/auto_entitylabel": "^3.0@beta",
        "drupal/background_image": "^2.0",
        "drupal/background_image_formatter": "^1.7",
        "drupal/backup_migrate": "^5.0",
        "drupal/bg_image_formatter": "^1.15",
        "drupal/bg_img_field": "^1.7",
        "drupal/block_list_override": "^1.0",
        "drupal/block_permissions": "^1.2",
        "drupal/bootstrap_barrio": "^5.1",
        "drupal/bootstrap_layouts": "^5.2",
        "drupal/bootstrap_paragraphs": "^2.0@beta",
        "drupal/business_rules": "^2.0@beta",
        "drupal/calendar": "^1.0@alpha",
        "drupal/ckeditor_bootstrap_table": "^1.2",
        "drupal/ckeditor_font": "^1.2",
        "drupal/color_field": "^2.5",
        "drupal/colorbox": "^1.7",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/config_ignore": "^2.3",
        "drupal/config_split": "^2.0@beta",
        "drupal/config_terms": "^1.5",
        "drupal/context": "^4.0@beta",
        "drupal/context_entity_field": "1.x-dev@dev",
        "drupal/core-composer-scaffold": "^8.8",
        "drupal/core-project-message": "^8.8",
        "drupal/ctools": "^3.7",
        "drupal/devel": "^4.1",
        "drupal/disable_messages": "^2.1@alpha",
        "drupal/display_field_copy": "^2.0",
        "drupal/ds": "^3.13",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/entity_redirect": "^2.0",
        "drupal/entity_reference_revisions": "^1.9",
        "drupal/entity_submit_redirect": "1.x-dev@dev",
        "drupal/eva": "^2.1",
        "drupal/feeds": "^3.0@alpha",
        "drupal/feeds_ical": "^1.2",
        "drupal/feeds_tamper": "^2.0@beta",
        "drupal/field_group": "3.x-dev@dev",
        "drupal/field_group_link": "^3.0@RC",
        "drupal/field_permissions": "^1.1",
        "drupal/fitvids": "^1.1",
        "drupal/flood_control": "^2.2",
        "drupal/focal_point": "^1.5",
        "drupal/fontyourface": "^3.6",
        "drupal/fullcalendar_view": "^5.0",
        "drupal/google_analytics": "^3.1",
        "drupal/gtranslate": "^1.14",
        "drupal/image_effects": "^3.1",
        "drupal/imagefield_slideshow": "^2.0",
        "drupal/imagemagick": "^3.2",
        "drupal/libraries": "^3.0@beta",
        "drupal/linkit": "^6.0@beta",
        "drupal/maxlength": "^2.0@RC",
        "drupal/menu_block": "^1.7",
        "drupal/onlyone": "^1.10",
        "drupal/paragraph_blocks": "^3.0",
        "drupal/paragraph_view_mode": "^2.0",
        "drupal/paragraphs": "^1.12",
        "drupal/paragraphs_edit": "^2.0@alpha",
        "drupal/password_policy": "^3.0",
        "drupal/pathauto": "^1.8",
        "drupal/redis": "^1.5",
        "drupal/required_api": "^1.3",
        "drupal/required_by_role": "^1.1",
        "drupal/scheduler": "^1.4",
        "drupal/smart_date": "^3.3",
        "drupal/smart_trim": "^1.3",
        "drupal/social_media_links": "^2.8",
        "drupal/tabtamer": "^2.0@beta",
        "drupal/taxonomy_manager": "^2.0",
        "drupal/term_merge": "1.x-dev@dev",
        "drupal/theme_permission": "^2.0@beta",
        "drupal/token": "^1.9",
        "drupal/twitter_block": "^3.0@alpha",
        "drupal/unpublished_paragraphs": "^1.2",
        "drupal/upgrade_status": "^3.11",
        "drupal/video_embed_field": "^2.4",
        "drupal/views_bootstrap": "^4.3",
        "drupal/views_bulk_edit": "^2.6",
        "drupal/views_bulk_operations": "^4.0",
        "drupal/views_conditional": "^1.4",
        "drupal/views_ical": "1.x-dev@dev",
        "drupal/views_menu_children_filter": "^1.0@RC",
        "drupal/views_sort_options_weight": "1.x-dev@dev",
        "drupal/views_year_filter": "^1.7",
        "drupal/webform": "^6.0",
        "drupal/workbench": "^1.3",
        "drupal/workbench_access": "^1.0@beta",
        "drupal/workbench_menu_access": "^1.3",
        "drush/drush": "^10.4.3",
        "onlyextart/colorbox": "9999999-dev",
        "php-ews/php-ews": "^1.0",
        "platformsh/config-reader": "^2.4",
        "recurser/jquery-simple-color": "^1.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "patches": {
          "drupal/field_group": {
              "Adds tokens to html_element groups": "https://www.drupal.org/files/issues/2020-07-03/2961885-13.patch"
          },
          "drupal/feeds": {
              "Add a mapping target to media field": "https://www.drupal.org/files/issues/2021-08-22/feeds-media-image-2928904.patch"
          },
          "drupal/core": {
              "Move cursor to new line after image insert in CKEditor": "https://www.drupal.org/files/issues/2019-12-18/0004-Set-focus-for-chrome-after-html-insert-with-space.patch"
          }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
